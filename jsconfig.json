{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es6",
    "paths": {
      "components": ["src/components"],
      "assets": ["src/assets"],
      "pages": ["src/pages"],
      "@react-router/dev/routes": ["./node_modules/@react-router/dev/dist/routes.d.ts"],
      "react-router": ["./node_modules/react-router/dist/index.d.ts"]
    }
  },
  "include": ["src", "src/**/*"],
  "exclude": ["node_modules"]
}